{
  "name": "bare-build",
  "version": "0.2.7",
  "description": "Application builder for Bare",
  "exports": {
    "./package": "./package.json",
    ".": "./index.js"
  },
  "imports": {
    "child_process": {
      "bare": "bare-subprocess",
      "default": "child_process"
    },
    "fs": {
      "bare": "bare-fs",
      "default": "fs"
    },
    "os": {
      "bare": "bare-os",
      "default": "os"
    },
    "path": {
      "bare": "bare-path",
      "default": "path"
    },
    "url": {
      "bare": "bare-url",
      "default": "url"
    }
  },
  "bin": {
    "bare-build": "bin.js"
  },
  "files": [
    "index.js",
    "bin.js",
    "CMakeLists.txt",
    "lib",
    "prebuilds"
  ],
  "scripts": {
    "test": "prettier . --check && bare test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holepunchto/bare-build.git"
  },
  "author": "Holepunch",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/holepunchto/bare-build/issues"
  },
  "homepage": "https://github.com/holepunchto/bare-build#readme",
  "dependencies": {
    "bare-bundle-id": "^1.0.2",
    "bare-fs": "^4.5.1",
    "bare-lief": "^0.1.3",
    "bare-link": "^2.1.2",
    "bare-module-resolve": "^1.12.0",
    "bare-module-traverse": "^1.8.1",
    "bare-os": "^3.6.2",
    "bare-pack": "^1.5.1",
    "bare-path": "^3.0.0",
    "bare-subprocess": "^5.1.5",
    "bare-unpack": "^1.1.3",
    "bare-url": "^2.3.2",
    "paparam": "^1.8.0",
    "require-asset": "^1.1.0"
  },
  "optionalDependencies": {
    "bare-apk": "^0.1.1",
    "bare-app-image": "^0.1.0"
  },
  "devDependencies": {
    "bare-bundle": "^1.10.0",
    "bare-module": "^6.1.2",
    "brittle": "^3.18.0",
    "cmake-bare": "^1.6.5",
    "cmake-bare-bundle": "^2.1.4",
    "cmake-fetch": "^1.4.0",
    "prettier": "^3.4.1",
    "prettier-config-holepunch": "^2.0.0"
  }
}
